@model SalesWebMVC.Models.Seller
@{
    ViewData[""] = "Cadastro";
}
<h2>
    @ViewData["Title"]</h2>

<h4>Seller</h4>
<hr/>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Name" class="control-label">Nome</label>
                <input asp-for="Name" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Mail" class="control-label">E-Mail</label>
                <input asp-for="Mail" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="BithDate" class="control-label">Aniversário</label>
                <input asp-for="BithDate" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="BaseSalary" class="control-label">Base Salário</label>
                <input asp-for="BaseSalary" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

